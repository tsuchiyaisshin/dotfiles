# npm
alias ntc='npm run test:c'

# rails
alias nimas='CORS_ALLOWED_ORIGINS=dynamic bin/rails s'
alias nimaj='bin/rails jobs:work'
alias rbcop='bundle exec rubocop -a'

# overwrap
alias lf='ls -a -lF | grep -v /'

